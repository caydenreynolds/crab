///
/// This struct represents a 64 bit unsigned integer
/// Note that the Int type will later be promoted to an interface
///
struct Int {
    __uint64__ value
}

intr Int {
   Printable,
   Addable,
}

impl Int {
    fn operatorAdd(Int other) -> Int {
        return Int {
            value = __add_int__(self.value, other.value)
        }
    }

    fn toString() -> String {
        return __format_i__(self.value)
    }
}
